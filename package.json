  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.11.7",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "mock-fs": "^5.1.2",
    "rimraf": "^3.0.2",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.4.4",
    "pkg": "^5.4.1"
  },
  "pkg": {
    "path": "build/index.js",
    "outputPath": "distribution/",
    "compress": "Brotli"
